{
  "name": "preact-custom-element",
  "version": "4.0.0",
  "description": "Wrap your component up as a custom element",
  "main": "dist/preact-custom-element.js",
  "module": "dist/preact-custom-element.esm.js",
  "unpkg": "dist/preact-custom-element.umd.js",
  "source": "src/index.js",
  "scripts": {
    "prepare": "microbundle",
    "test": "karmatic",
    "test:browser": "MOZ_HEADLESS=1 karmatic --no-coverage --browsers chrome:headless,firefox,sauce-safari",
    "prettier": "prettier **/*.js --write"
  },
  "eslintConfig": {
    "extends": "developit"
  },
  "repository": "preactjs/preact-custom-element",
  "keywords": [
    "preact",
    "web",
    "components",
    "custom",
    "element"
  ],
  "authors": [
    "Bradley J. Spaulding",
    "The Preact Authors (https://preactjs.com)"
  ],
  "license": "MIT",
  "bugs": "https://github.com/preactjs/preact-custom-element/issues",
  "homepage": "https://github.com/preactjs/preact-custom-element",
  "peerDependencies": {
    "preact": "^10"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-developit": "^1.1.1",
    "karmatic": "^1.4.0",
    "microbundle": "^0.12.0-next.6",
    "mocha": "^6.2.2",
    "preact": "^10.1.1",
    "prettier": "^1.19.1",
    "webpack": "^4.41.3"
  }
}
