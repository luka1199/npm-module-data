{
  "name": "named-regexp-groups",
  "version": "1.0.0",
  "description": "Regular expressions with named capture groups and named back-references",
  "main": "lib",
  "jsnext:main": "src",
  "keywords": [
    "regular",
    "expression",
    "regex",
    "regexp",
    "named",
    "capture",
    "group",
    "backreference"
  ],
  "homepage": "https://github.com/commenthol/named-regexp-groups/",
  "bugs": {
    "url": "https://github.com/commenthol/named-regexp-groups/issues"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/commenthol/named-regexp-groups.git"
  },
  "scripts": {
    "__prepublish": "npm run clean && npm run lint && npm run build && npm test",
    "test": "mocha",
    "coverage": "nyc --reporter=html --reporter=text mocha",
    "build": "babel -d lib src",
    "lint": "eslint '**/*.js'",
    "clean": "rimraf lib coverage"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "core-js": "^2.4.1",
    "mocha": "*",
    "rimraf": "*",
    "safe-regex": "^1.1.0"
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ],
    "rules": {}
  },
  "babel": {
    "ignore": [
      "node_modules"
    ],
    "presets": [
      "es2015",
      "stage-0",
      "stage-1",
      "stage-2"
    ]
  }
}
