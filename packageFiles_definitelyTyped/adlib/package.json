{
  "name": "adlib",
  "version": "3.0.0",
  "description": "Templating for deep JSON object graphs",
  "main": "dist/adlib.js",
  "module": "lib/index.js",
  "unpkg": "dist/adlib.umd.js",
  "files": [
    "dist/**",
    "lib/**"
  ],
  "scripts": {
    "start": "npm run build && concurrently \"watch 'npm run build' lib\" \"mocha -w\"",
    "init": "mkdir dist",
    "clean": "rm -rf dist",
    "build": "rollup -c profiles/dev.js",
    "build:release": "rollup -c profiles/umd.js && rollup -c profiles/prod.js && rollup -c profiles/dev.js",
    "test": "node test/duct.js",
    "test:compiled": "mocha test/*.spec.js",
    "chk": "yarn test && yarn build:release && yarn test:compiled"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/adlib.git"
  },
  "keywords": [
    "arcgis",
    "json",
    "templating"
  ],
  "author": {
    "name": "Dave Bouwman",
    "email": "dbouwman@esri.com"
  },
  "contributors": [
    {
      "name": "Andrew Turner",
      "email": "aturner@esri.com"
    },
    {
      "name": "Tom Wayson",
      "email": "twayson@esri.com"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Esri/adlib/issues"
  },
  "homepage": "https://arcgis.github.io/ember-arcgis-adlib-service",
  "@std/esm": {
    "esm": "all"
  },
  "dependencies": {
    "esm": "^3.0.49"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-function-name": "^6.24.1",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.2",
    "concurrently": "^3.4.0",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-semistandard": "~12.0.1",
    "eslint-config-standard": "~11.0.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^5.2.0",
    "rollup": "^1.0.1",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^6.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "tape": "^4.8.0",
    "watch": "^1.0.2"
  }
}
