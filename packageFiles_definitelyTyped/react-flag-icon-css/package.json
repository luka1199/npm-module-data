{
  "name": "react-flag-icon-css",
  "version": "1.0.0",
  "description": "React SVG country flags component",
  "main": "./lib",
  "scripts": {
    "clean": "rimraf ./lib",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir ./lib --ignore **/__tests__/",
    "postbuild": "npm run sass && flow-copy-source -v src lib",
    "prepublish": "npm run test && npm run build",
    "sass": "mkdirp lib/sass && cp src/sass/*.scss lib/sass",
    "test": "tape -r babel-register \"src/**/__tests__/index.js\"",
    "posttest": "npm run lint && npm run flow",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src cmrh.conf.js",
    "lint:css": "sass-lint 'src/**/*.scss' -v -q"
  },
  "author": {
    "name": "matteocng",
    "email": "matteocng+npmjs@outlook.com",
    "url": "https://github.com/matteocng"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteocng/react-flag-icon-css.git"
  },
  "bugs": {
    "url": "https://github.com/matteocng/react-flag-icon-css/issues"
  },
  "keywords": [
    "react",
    "flag-icon",
    "flag-icons",
    "flag-icon-css",
    "react-css-modules"
  ],
  "peerDependencies": {
    "react": "^0.14 || ^15.0",
    "react-dom": "^0.14 || ^15.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react-css-modules": "^4.0.0",
    "tcomb": "^3.2.15",
    "tcomb-react": "^0.9.3",
    "flag-icon-css": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "classnames": "^2.2.5",
    "css-modules-require-hook": "^4.0.3",
    "enzyme": "^2.4.1",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.19.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "flag-icon-css": "^2.4.0",
    "flow-bin": "^0.32.0",
    "flow-copy-source": "^1.1.0",
    "jsdom": "^9.5.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.10.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-css-modules": "^4.0.0",
    "react-dom": "^15.3.2",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.9.1",
    "tape": "^4.6.0",
    "tcomb": "^3.2.15",
    "tcomb-react": "^0.9.3"
  }
}
