{
  "name": "use-color-scheme",
  "version": "1.1.3",
  "description": "React Hook to get users prefered color scheme",
  "keywords": [
    "react",
    "hooks",
    "react hooks",
    "dark mode",
    "light mode",
    "prefers-color-scheme",
    "color-scheme"
  ],
  "homepage": "https://github.com/jcblw/use-color-scheme#readme",
  "repository": {
    "type": "git",
