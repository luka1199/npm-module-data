    "grunt-contrib-copy": "latest",
    "grunt-contrib-qunit": "latest",
    "grunt-contrib-uglify": "^4.0.1",
    "grunt-mocha-nyc": "latest",
    "grunt-release": "github:davidbau/grunt-release",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "qunit": "latest",
    "requirejs": "latest"
  }
