{
  "name": "sudokus",
  "version": "1.0.0",
  "description": "A JavaScript utility to solve sudokus.",
  "keywords": [
    "sudoku",
    "puzzle",
    "solver"
  ],
  "author": "Dieter Luypaert <dieterluypaert@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:Moeriki/node-sudokus.git",
  "engines": {
    "node": ">=6"
  },
  "main": "lib/index.js",
  "bin": {
    "sudokus": "cli.js"
  },
  "scripts": {
    "lint": "eslint *.js lib/",
    "test": "jest"
  },
  "dependencies": {
    "chalk": "~1.1.3",
    "lodash": "4.17.2",
    "log-update": "~1.0.2",
    "table": "~4.0.1",
    "yargs": "~6.5.0"
  },
  "devDependencies": {
    "eslint": "3.10.0",
    "eslint-config-muriki": "7.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jasmine": "~2.2.0",
    "eslint-plugin-node": "3.0.4",
    "jest": "~18.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**.js"
    ],
    "testEnvironment": "node",
    "testRegex": "/test/.*\\.test\\.js$"
  }
}
