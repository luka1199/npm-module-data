{
  "name": "react-motion-slider",
  "version": "0.4.1",
  "description": "Slider/Carousel powered by React Motion.",
  "main": "lib/react-motion-slider.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build": "npm run build:lib && NODE_ENV=production webpack --config webpack.prod.config.js",
    "dev": "webpack-dev-server --devtool eval --hot --progress --colors",
    "postbuild": "NODE_ENV=production TARGET=minify webpack --config webpack.prod.config.js",
    "prebuild": "rm -rf dist && mkdir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/souporserious/react-motion-slider"
  },
  "keywords": [
    "react",
    "component",
    "slider",
    "carousel"
  ],
  "author": "Travis Arnold <travis@souporserious.com> (http://souporserious.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/souporserious/react-motion-slider/issues"
  },
  "homepage": "https://github.com/souporserious/react-motion-slider",
  "dependencies": {
    "get-prefix": "^1.0.0",
    "is-integer": "^1.0.6",
    "react-motion": "^0.4.3"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "alt": "^0.17.3",
    "autoprefixer-loader": "^2.0.0",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.15.1",
    "http-server": "^0.8.0",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "react": "^15.1.0",
    "react-collapse": "^2.2.3",
    "react-dom": "^15.1.0",
    "react-height": "^2.1.0",
    "react-measure": "^0.3.5",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.12",
    "webpack-dev-server": "^1.9.0"
  }
}
