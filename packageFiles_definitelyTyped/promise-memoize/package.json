  },
  "devDependencies": {
    "any-promise": "^1.3.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "coveralls": "^2.11.9",
    "eslint": "~2.13.1",
    "istanbul": "^0.4.3",
    "lie": "^3.0.4",
    "mocha": "^2.5.3"
